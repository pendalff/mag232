<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <referenceContainer name="page.wrapper">
	
		<referenceContainer name="header.container" htmlClass="header" />
		<referenceContainer name="footer-container" htmlClass="footer" />
		
		<block class="Magento\Framework\View\Element\Template" name="mgs.script" template="Magento_Theme::script.phtml" after="footer-container"/>
		
    </referenceContainer>
</layout>
